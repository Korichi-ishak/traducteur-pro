{
  "name": "traducteur-pro",
  "version": "1.0.0",
  "description": "Application de traduction Allemand ⇄ Français avec Supabase",
  "scripts": {
    "install:all": "npm install --prefix server && npm install --prefix client",
    "dev:server": "cd server && node server.js",
    "dev:client": "cd client && npm run dev",
    "build:client": "cd client && npm run build",
    "preview:client": "cd client && npm run preview"
  },
  "keywords": ["traduction", "allemand", "français", "react", "node"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
